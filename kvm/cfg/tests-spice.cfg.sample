        qemu_io_binary = /usr/bin/qemu-io
        guest_vm = vm1
        client_vm = vm2
        display_vm2 = vnc
        vga_vm2 = cirrus
        migrate_background = yes
        iterations = 1
        kill_vm = no
        vms = vm1 vm2
